   cTxt += '[ "JPCADASTRO", "CDSTATUS", "JPTABCADSTA", "IDCADSTA", "CADSTANOME" ],' + hb_Eol()
   cTxt += '[ "JPTABCIDADE", "CIUF", "JPTABUF", "IDUF", "UFNOME" ],' + hb_Eol()
   cTxt += '[ "JPTABUF", "UFTRIUF", "JPTABTRIUF", "IDTRIUF", "TRIUFNOME" ],' + hb_Eol()
   cTxt += '[ "JPIMPOSTO", "IMTRANSACAO",  "JPTRANSACAO",  "IDTRANSACAO",  "TRNOME" ],' + hb_Eol()
   cTxt += '[ "JPIMPOSTO", "IMTRIUF",  "JPTABTRIUF",  "IDTRIUF",  "TRIUFNOME" ],' + hb_Eol()
   cTxt += '[ "JPIMPOSTO", "IMTRICAD", "JPTABTRICAD", "IDTRICAD", "TRICADNOME" ],' + hb_Eol()
   cTxt += '[ "JPIMPOSTO", "IMTRIPRO", "JPTABTRIPRO", "IDTRIPRO", "TRIPRONOME" ],' + hb_Eol()
