   cTxt += '[ "DBGROUP",     "IDGROUP",    2 ],' + hb_Eol()
   cTxt += '[ "DBSTOCK",     "IDSTOCK"       ],' + hb_Eol()
   cTxt += '[ "DBFINANC",    "IDFINANC"      ],' + hb_Eol()
   cTxt += '[ "DBSTATE",     "IDSTATE",    2 ],' + hb_Eol()
   cTxt += '[ "DBTICKET",    "IDTICKET"      ],' + hb_Eol()
